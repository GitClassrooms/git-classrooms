{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Classroom",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "type": "string"
    },
    "ownerId": {
      "type": "integer"
    },
    "description": {
      "type": "string"
    },
    "groupId": {
      "type": "integer"
    }
  },
  "required": ["id", "name", "ownerId", "description", "groupId"]
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreateClassroomRequest",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "memberEmails": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "email"
      }
    },
    "description": {
      "type": "string"
    }
  },
  "required": ["name", "memberEmails"]
}
